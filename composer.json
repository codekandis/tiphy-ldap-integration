{
	"name": "codekandis/tiphy-ldap-integration",
	"description": "`codekandis/tiphy-ldap-integration` is a library integrating `codekandis/ldap` into `codekandis/tiphy`.",
	"keywords": [
		"php",
		"library",
		"tiphy",
		"ldap",
		"integration"
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/tiphy-ldap-integration/issues",
		"docs": "https://github.com/codekandis/tiphy-ldap-integration/blob/master/README.md",
		"source": "https://github.com/codekandis/tiphy-ldap-integration/tree/master"
	},
	"require": {
		"php": "^7.4",
		"codekandis/configurations": "^0",
		"codekandis/ldap": "^0",
		"codekandis/tiphy": "^0"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"codekandis/phpunit": "^3"
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\TiphyLdapIntegration\\": [
				"src/"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\TiphyLdapIntegration\\Tests\\": [
				"tests/"
			]
		}
	},
	"scripts": {
		"post-update-cmd": []
	}
}
